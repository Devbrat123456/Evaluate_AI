<script>


    function messagePop(message, icon = "success") {
        // console.log(message);
        swal.fire({
            position: 'middle',
            icon: icon,
            title: message,
            showConfirmButton: true,
            timer: 60000
        });
    }

    $(".delete-button").on("click", function () {
        var id = this.id.trim();
        const table_name = $(this).attr('table');
        var row = $(this).closest('tr');

        Swal.fire({
            title: "Are you sure?",
            text: "You won't be able to revert this!",
            type: "warning",
            showCancelButton: !0,
            confirmButtonColor: "#3085d6",
            cancelButtonColor: "#d33",
            confirmButtonText: "Yes, delete it!",
            confirmButtonClass: "btn btn-primary",
            cancelButtonClass: "btn btn-danger m-3",
            buttonsStyling: !1,
        }).then(function (t) {
            console.log(t, t.value);
            if (t.value) {

                var url = `${table_name}/${id}`;
                $.ajaxSetup({
                    headers: {
                        'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
                    }
                });
                $.ajax({
                    type: "DELETE",
                    url: url,
                    success: function (data) {

                        if (data.success) {

                            t.value && Swal.fire({
                                icon: "success",
                                title: "Deleted!",
                                text: data.message || "Your data has been deleted.",
                                confirmButtonClass: "btn btn-success",
                            }).then(function () {

                                row.remove();
                            });
                        } else {
                            Swal.fire({
                                icon: "error",
                                title: "Not Deleted!",
                                text: data.message || "Error Found. Please check and retry.",
                                confirmButtonClass: "btn btn-danger",
                            });
                        }
                    },

                    error: function () {
                        t.value &&
                            Swal.fire({
                                type: "Error",
                                title: "Deleted!",
                                text: "Error Found Please check and Re-Try.",
                                confirmButtonClass: "btn btn-success",
                            });
                    }
                });
            }


        });
    })
</script>
