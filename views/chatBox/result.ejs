<%- include('../includes/header') %>
    <style>
        #form-container {
            margin-top: 100px !important;
            ;

        }

        .form-detail img {
            width: 70%;
        }

        .button {

            display: flex;
            justify-self: end;

        }

        .button input {
            margin: reset !important;
        }
    </style>
    <main>
        <div class="row">
            <div class="col-md-12">
                <div class="form-v7">
                    <div class="page-content">
                        <div class="form-v7-content ">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="form-detail">
                                      <table class="table table-responsive table-bordered table-striped">

                                          <thead>
                                            <tr>

                                                <th>Question </th>
                                                <th>Answer </th>
                                                <th>Score </th>
                                                <th>Feedback</th>
                                            </tr>
                                          </thead>

                                                <% existingRecord.forEach((value)=>{
                                                         console.log(value)

                                                         %>
                                                                <tr>
                                                                    <td> <%-value.question %></td>
                                                                    <td> <%-value.answer %></td>
                                                                    <td> <%-value.score %></td>
                                                                    <td> <%-value.feedback %></td>
                                                                </tr>

                                                         <%
                                                      
                                                }) %>
                                      </table>

                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        </div>
    </main>

    <%- include('../includes/footer') %>